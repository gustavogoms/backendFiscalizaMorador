{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [

"plugin:@typescript-eslint/recommended",
        "airbnb-base",
"plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {

"prettier/prettier": "error",
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "ts": "never"
        }
      ],
      "no-use-before-define": "off",
"@typescript-eslint/no-use-before-define": [
	"error"
]
    },
    "settings": {
      "import/resolver": {
        "typescript": {}
      }
    }
}
